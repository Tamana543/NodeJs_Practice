<main>
     <% if(order <=0 ) {%>
     <h1>Nothing to be ordered</h1>
     <% }else{%>
          <ul>
               <% order.forEach(item =>{ %>
               <li>

                    <h2> <% order.id %></h2>
                    <ul>
                         <% order.orderItem.forEach(item => { %>

                         <li>
                              <%= item.title %> ( <%= item.quntity %>)
                         </li>
                         <% }); %>
                    </ul>
               </li>
               <% }) ;%>
          </ul>
     <%} %>     
</main>
